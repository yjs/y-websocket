{
  "name": "@y/websocket",
  "version": "4.0.0-rc.0",
  "description": "Websockets provider for Yjs",
  "main": "./dist/y-websocket.cjs",
  "module": "./src/y-websocket.js",
  "types": "./dist/src/y-websocket.d.ts",
  "type": "module",
  "sideEffects": false,
  "funding": {
    "type": "GitHub Sponsors ‚ù§",
    "url": "https://github.com/sponsors/dmonad"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "dist": "npm run clean && rollup -c && tsc --skipLibCheck",
    "lint": "standard && tsc --skipLibCheck",
    "test": "node ./test/index.js",
    "preversion": "npm run dist && test -e dist/y-websocket.d.ts && test -e dist/y-websocket.cjs"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "module": "./src/y-websocket.js",
      "import": "./src/y-websocket.js",
      "require": "./dist/y-websocket.cjs",
      "types": "./dist/y-websocket.d.ts",
      "default": "./src/y-websocket.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yjs/y-websocket.git"
  },
  "keywords": [
    "Yjs"
  ],
  "author": "Kevin Jahns <kevin.jahns@protonmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yjs/y-websocket/issues"
  },
  "homepage": "https://github.com/yjs/y-websocket#readme",
  "standard": {
    "ignore": [
      "/dist",
      "/node_modules"
    ]
  },
  "dependencies": {
    "@y/protocols": "^1.0.6-rc.1",
    "lib0": "^1.0.0-rc.1"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@y/websocket-server": "^0.1.5",
    "@y/y": "^14.0.0-22",
    "rollup": "^4.43.0",
    "standard": "^17.1.2",
    "typescript": "^5.8.3",
    "ws": "^8.19.0",
    "yjs": "^13.6.29"
  },
  "peerDependencies": {
    "@y/y": "*"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  }
}
